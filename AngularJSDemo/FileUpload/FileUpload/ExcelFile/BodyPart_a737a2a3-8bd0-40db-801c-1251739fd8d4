create table tb_Order                         --订单表
(
	OrderID int identity(10000000,1) not null,--订单id
	UserName nvarchar(50) not null,           --用户名
	RealName nvarchar(50) not null,           --真实姓名
	Address nvarchar(200) not null,           --详细地址
	phonenumber int not null,
	ProductID int not null,                   --产品ID
	BrandName nvarchar(50) not null,          --品牌
	ProductName nvarchar(50) not null,        --型号
	ProductColor nvarchar(10) not null,       --产品颜色
	Capacity varchar(10) not null,            --容量
	Price float not null,                     --单价
	Quantity int not null,                    --数量
	Express nvarchar(50) not null,            --配送方式
	OrderStatus nvarchar(50) not null,        --订单状态
	LoadDate datetime not null,                --创建时间
	--ShoppingID int ,
)
go
drop table tb_Order
alter table tb_Order add constraint PK_OrderID primary key(OrderID),
						 constraint Fk_OuserName Foreign key(UserName) references tb_User(UserName),
						 constraint Ck_Oquantity check(Quantity>0),
						 constraint DF_Express default(N'顺丰') for Express,
						 constraint DF_OrderStatus default(N'未付款') for OrderStatus,
						 constraint DF_OLoadDate default(getdate()) for LoadDate
						-- constraint FK_ShoppingID foreign key(ShoppingID) references tb_shopcart(ShoppingID)
						 

create table tb_Advise             --建议表
(
  id int primary key identity(1,1),--建议id
  name nvarchar(50) not null,      --姓名
  phone varchar(50) not null,      --电话
  email varchar(20) not null,      --Email
  message nvarchar(280) not null,  --建议内容
  LoadDate datetime not null       --创建时间
  )

  alter table tb_Advise add 
							constraint DF_AdloadDate default(getdate()) for LoadDate


