  use B2Cdata
go

create proc proc_GCN
(
	@ClassID int
)
as
if exists(select * from tb_Class where ClassID=@ClassID)
	begin
		select ClassName from tb_Class where ClassID=@ClassID
	end
go

create proc proc_PWD
(
	@UserName nvarchar(50)
)
as
if exists(select * from tb_User where UserName=@UserName)
	begin
		select Password from tb_User where UserName=@UserName
	end
go

create proc proc_Phone
(
	@UserName nvarchar(50)
)
as
if exists(select * from tb_User where UserName=@UserName)
	begin
		select Phonecode from tb_User where UserName=@UserName
	end
go

create proc proc_Email
(
	@UserName nvarchar(50)
)
as
if exists(select * from tb_User where UserName=@UserName)
	begin
		select Email from tb_User where UserName=@UserName
	end
go
