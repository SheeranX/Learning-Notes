insert into tb_order(username,realname,address,productid,brandname,productname,productcolor,capacity,price,Quantity)

select tb_product.productid,tb_product.brandname,tb_product.productname,tb_product.productcolor,tb_product.capacity,tb_product.hotprice, tb_shopcart.quantity from tb_product ,tb_shopcart where tb_product.productid = tb_shopcart.productid


