create database B2Cdata
go

use B2Cdata
go

create table tb_User                           --会员账号表
(
	UserName nvarchar(50) not null,            --登录名
	Password varchar(50) not null,             --登录密码
	Phonecode varchar(20) not null,            --电话号码
	Email varchar(50) not null,                --E-mail地址
)

alter table tb_User add constraint PK_UserName primary key(UserName)

create table tb_Member                         --会员信息表
(
	MemberID int identity(20160001,1) not null,--ID（自增主键）
	UserName nvarchar(50) not null,            --登录名
	RralName nvarchar(50) not null,            --真实姓名
	Sex nvarchar(2) not null,                  --性别
	Address nvarchar(200) not null,            --详细地址
	PsotCode varchar(10) not null,             --邮编
	LoadDate datetime not null                 --创建时间
)
go

alter table tb_Member add constraint PK_MemberID primary key(MemberID),
						  constraint FK_UserName foreign key(UserName) references tb_User(UserName),
						  constraint DF_Sex default('男') for Sex,
						  constraint DF_LoadDate default(getdate()) for LoadDate

create table tb_Admin                  --管理员信息表
(
	AdminID int identity(1,1) not null,--ID(自增主键)
	AdminName nvarchar(50) not null,   --登录名
	Password varchar(50) not null,     --密码
	RealName nvarchar(50) not null,    --真实姓名
	Email varchar(50) not null,        --E-mail地址
	LoadDate datetime not null         --创建时间
)
go

alter table tb_Admin add  constraint PK_AdminID primary key(AdminID),
						  constraint DF_ALoadDate default(getdate()) for LoadDate

create table tb_Class              --产品类型表
(
	ClassID int not null,          --类型ID
	ClassName nvarchar(50) not null--类型名
)
go

alter table tb_Class add constraint PK_ClassID primary key(ClassID)

create table tb_Product                     --产品信息表
(
	ProductID int identity(10000,1) not null,--产品ID
	ClassID int not null,                    --类型ID
	BrandName nvarchar(50) not null,         --品牌
	ProductName nvarchar(50) not null,       --型号
	ProductType nvarchar(50) not null,       --种类
	ProductInfo ntext not null,              --详细信息
	ProductURL nvarchar(200) not null,       --图片地址
	ProductColor nvarchar(10) not null,      --产品颜色
	Capacity varchar(10) not null,           --容量
	MarketPrice float not null,              --市场价
	HotPrice float not null,                 --热销价
	LoadDate datetime not null,              --进货时间
	Storage int not null                     --库存
)
go
--drop table tb_Product
alter table tb_Product add constraint PK_ProductID primary key(ProductID),
						   constraint FK_ClassID foreign key(ClassID) references tb_Class(ClassID),
						   constraint DF_PLoadDate default(getdate()) for LoadDate,
						   constraint CK_Storage check(Storage>=0)



create table tb_LeaveWord          --会员评价表
(
	ID int identity(1,1) not null, --ID(自增主键)
	UserName nvarchar(50) not null,--用户名
	ProductID int not null,        --产品ID
	Uid nvarchar(50) not null,     --评价人用户名
	content ntext not null,        --内容
	DateTime datetime not null     --评价时间
)
go

alter table tb_LeaveWord add  constraint PK_ID primary key(ID)
alter table tb_LeaveWord add  constraint DF_DateTime default(getdate()) for DateTime
alter table tb_LeaveWord add constraint  FK_Prouctid foreign key(productid) references tb_product(productid)
select * from tb_LeaveWord
insert into tb_LeaveWord(username,productid,uid,content)values(N'qqqqqq',10000,N'uiui',N'jjjjjjj')
--create table tb_Reply--回复评价表
--(
--	ID int identity(1,1) not null,--ID(自增主键)
--	Uname nvarchar(50) not null,--回复人用户名
--	content ntext not null,--内容
--	DateTime datetime not null--评价时间
--)
--go

--alter table tb_Reply add  constraint PK_RID primary key(ID),
--						  constraint DF_RDateTime default(getdate()) for DateTime

create table tb_shopCart                  --购物车表
(
	ShoppingID int identity(1,1) not null,--购物车ID
	UserName nvarchar(50) not null,       --用户名
	ProductID int not null,               --产品ID
	BrandName nvarchar(50) not null,      --品牌
	ProductName nvarchar(50) not null,    --型号
	Quantity int not null,                --数量
	Price float not null,                  --单价
    imageurl varchar(50) not null
)
go
drop table tb_shopCart 
alter table tb_shopCart add constraint PK_ShoppingID primary key(ShoppingID),
						  -- constraint FK_SuserName foreign key(UserName) references tb_User(UserName),
						   constraint FK_ProductID foreign key(ProductID) references tb_Product(ProductID),
						   constraint CK_Quantity check(Quantity>0),
						   constraint DF_Username default('null') for username,
						   constraint DF_Quantity default(1) for Quantity
----------------------------
--//////*存贮过程*//////----
----------------------------
create proc pro_insert 
(
  @ProductID int 
)
as 
INSERT INTO tb_shopCart
    (ProductID,BrandName,ProductName,imageurl,Price)
SELECT
    Productid,BrandName,ProductName,ProductURL, MarketPrice 
FROM tb_Product where productid = @ProductID 
go
--exec pro_insert 10000
drop proc pro_insert
select * from tb_shopcart
select brandname,productname,hotprice,producturl from tb_product where productid = @productid;




create table tb_Picture                  --图片表
(
	PictureID int identity(1,1) not null,--图片ID
	ProductID int not null,              --产品ID
	SubImage1 nvarchar(200) not null,    --图片地址1
	SubImage2 nvarchar(200) not null,    --图片地址2
	SubImage3 nvarchar(200) not null,    --图片地址3
	SubImage4 nvarchar(200) not null     --图片地址4
)
go

alter table tb_Picture add constraint PK_PictureID primary key(PictureID),
alter table tb_Picture add constraint FK_PproductID foreign key(ProductID) references tb_Product(ProductID)
alter table tb_Picture add constraint UQ_Productid UNIQUE(productid)

--drop table tb_Shopping
--drop table tb_Picture

